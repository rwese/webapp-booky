# Test Database (separate from development)
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/booky_test?schema=public"

# Authentication (use different secret for testing)
AUTH_SECRET="test-jwt-secret-key-change-in-production"
AUTH_JWT_EXPIRES_IN="1h"
AUTH_REFRESH_TOKEN_EXPIRES_IN="1d"
AUTH_PASSWORD_RESET_EXPIRES_IN="15m"
AUTH_BCRYPT_ROUNDS="4"

# Application
PORT=3001
APP_URL="http://localhost:3000"

# Test OAuth Providers (disabled by default)
AUTH_ENABLE_GOOGLE="false"
AUTH_ENABLE_GITHUB="false"
AUTH_ENABLE_DISCORD="false"
AUTH_ENABLE_APPLE="false"
